{
  "globalHeaders": {
    "cache-control": "no-cache, no-store, must-revalidate"
  },
  "routes": [
    {
      "route": "/*",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "route": "/index.html",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/api/*"]
  }
}
